{
  "name": "ngx-manager-with-bootstrap",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm i && ng serve --port=4200 --open --configuration=local",
    "build": "ng build",
    "build:prod": "ng build --base-href=/ --prod --build-optimizer=true --vendor-chunk=true --configuration=live",
    "build:github": "npm run build:prod -- --base-href=/ngx-manager-with-bootstrap/ --configuration=dev",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer dist/stats.json",
    "test": "jest",
    "updateSnapshot": "npm test -- --updateSnapshot",
    "coverage": "npm test -- --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "test:ci": "CI=true node --max_old_space_size=1024 ./node_modules/jest/bin/jest.js --maxWorkers=2",
    "format:check": "prettier --no-semi true --single-quote true --check \"src/app/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:write": "npm run format:check -- --write",
    "deploy:gh-pages": "gh-pages -d dist -m 'chore: update [skip ci]' -t",
    "compodoc": "compodoc -p src/tsconfig.app.json -s",
    "build:compodoc": "compodoc -p src/tsconfig.app.json -d dist/compodoc",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o dist/story"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@ng-select/ng-select": "^2.20.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.3",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^4.2.0",
    "ngx-order-pipe": "^2.0.2",
    "ngx-spinner": "^7.1.4",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.2",
    "@angular/cli": "^8.0.2",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@babel/core": "^7.4.5",
    "@compodoc/compodoc": "^1.1.9",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addon-notes": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/angular": "^5.1.3",
    "@storybook/cli": "^5.1.3",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "~2.0.6",
    "@types/jest": "^24.0.13",
    "@types/node": "^11.13.13",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.0.1",
    "gh-pages": "^2.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "~24.7.1",
    "jest-preset-angular": "^7.1.1",
    "prettier": "^1.18.2",
    "protractor": "~5.4.2",
    "ts-node": "^8.1.1",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ]
  }
}
