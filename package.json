{
  "name": "ngx-manager-with-bootstrap",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm i && ng serve --port=4200 --open --configuration=local",
    "build": "ng build",
    "build:prod": "ng build --base-href=/ --prod --build-optimizer=true --vendor-chunk=true --configuration=live",
    "build:github": "npm run build:prod -- --base-href=/ngx-manager-with-bootstrap/ --configuration=dev",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "test:ci": "CI=true node --max_old_space_size=1024 ./node_modules/jest/bin/jest.js --maxWorkers=2",
    "format:check": "prettier --no-semi true --single-quote true --check \"src/app/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:write": "npm run format:check -- --write",
    "deploy:gh-pages": "gh-pages -d dist -m 'chore: update [skip ci]' -t",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o dist/story"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.14",
    "@angular/compiler": "~7.2.14",
    "@angular/core": "~7.2.14",
    "@angular/forms": "~7.2.14",
    "@angular/platform-browser": "~7.2.14",
    "@angular/platform-browser-dynamic": "~7.2.14",
    "@angular/router": "~7.2.14",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^4.1.1",
    "ngx-order-pipe": "^2.0.2",
    "ngx-spinner": "^7.1.4",
    "rxjs": "~6.5.1",
    "tslib": "^1.9.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.14",
    "@angular/language-service": "~7.2.14",
    "@babel/core": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/angular": "^5.0.11",
    "@storybook/cli": "^5.0.11",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.10",
    "babel-loader": "^8.0.5",
    "codelyzer": "~5.0.1",
    "gh-pages": "^2.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "~24.7.1",
    "jest-preset-angular": "~7.1.0",
    "prettier": "^1.17.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ]
  }
}
